<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'>

    <ui:style>
        .container {
        margin: 10px;
        }

        .horizontalContainer {
        height:
        32px;
        }

        .right {
        float: right;
        text-align: right;
        margin-right: 10px;
        }

        .center {
        text-align: center
        }
    </ui:style>
    <g:HTMLPanel>
        <g:HorizontalPanel addStyleNames="{style.horizontalContainer}">
            <g:cell>
                <g:FlowPanel addStyleNames="{style.right}">
                     <g:HTML ui:field="processDefinitionIdLabel"></g:HTML>  
                </g:FlowPanel>
            </g:cell>
            <g:cell>
                <g:FlowPanel addStyleNames="{style.center}">
                    <g:HTML ui:field="processDefinitionIdText" />
                </g:FlowPanel>
            </g:cell>
        </g:HorizontalPanel>
        <g:HorizontalPanel addStyleNames="{style.horizontalContainer}">
            <g:cell>
                <g:FlowPanel addStyleNames="{style.right}">
                     <g:HTML ui:field="stateLabel"></g:HTML> 
                </g:FlowPanel>
            </g:cell>
            <g:cell>
                <g:FlowPanel addStyleNames="{style.center}">
                    <g:HTML ui:field="stateText" />
                </g:FlowPanel>
            </g:cell>
        </g:HorizontalPanel>
        <g:HorizontalPanel addStyleNames="{style.horizontalContainer}">
            <g:cell>
                <g:FlowPanel addStyleNames="{style.right}">
                     <g:HTML ui:field="processDeploymentLabel"></g:HTML> 
                </g:FlowPanel>
            </g:cell>
            <g:cell>
                <g:FlowPanel addStyleNames="{style.center}">
                    <g:HTML ui:field="processDeploymentText" />
                </g:FlowPanel>
            </g:cell>
        </g:HorizontalPanel>
        <g:HorizontalPanel addStyleNames="{style.horizontalContainer}">
            <g:cell>
                <g:FlowPanel addStyleNames="{style.right}">
                     <g:HTML ui:field="processVersionLabel"></g:HTML> 
                </g:FlowPanel>
            </g:cell>
            <g:cell>
                <g:FlowPanel addStyleNames="{style.center}">
                    <g:HTML ui:field="processVersionText" />
                </g:FlowPanel>
            </g:cell>
        </g:HorizontalPanel>
        <g:HorizontalPanel addStyleNames="{style.horizontalContainer}">
            <g:cell>
                <g:FlowPanel addStyleNames="{style.right}">
                     <g:HTML ui:field="correlationKeyLabel"></g:HTML> 
                </g:FlowPanel>
            </g:cell>
            <g:cell>
                <g:FlowPanel addStyleNames="{style.center}">
                    <g:HTML ui:field="correlationKeyText" />
                </g:FlowPanel>
            </g:cell>
        </g:HorizontalPanel>
        <g:HorizontalPanel addStyleNames="{style.horizontalContainer}">
            <g:cell>
                <g:FlowPanel addStyleNames="{style.right}">
                     <g:HTML ui:field="parentProcessInstanceIdLabel"></g:HTML> 
                </g:FlowPanel>
            </g:cell>
            <g:cell>
                <g:FlowPanel addStyleNames="{style.center}">
                    <g:HTML ui:field="parentProcessInstanceIdText" />
                </g:FlowPanel>
            </g:cell>
        </g:HorizontalPanel>
    </g:HTMLPanel>
</ui:UiBinder>